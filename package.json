{
  "name": "@fusion.io/framework",
  "version": "1.4.1",
  "description": "Fusion.IO Framework",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run build:watch\" \"npm run test:watch\"",
    "build": "babel src -d .",
    "build:watch": "nodemon --exec npm run build --watch src",
    "prepublish": "npm run build",
    "test:watch": "nodemon --exec npm run test --watch src --watch test",
    "test": "mocha --exit --require=@babel/register"
  },
  "author": "rikky <son.levuthai@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/preset-env": "^7.5.0",
    "@babel/register": "^7.4.4",
    "chai": "^4.2.0",
    "concurrently": "^4.1.1",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "sinon": "^7.3.2",
    "sqlite3": "^4.0.9"
  },
  "dependencies": {
    "@fusion.io/container": "^2.1.0",
    "bcrypt": "^3.0.6",
    "i18next": "^17.0.6",
    "i18next-sync-fs-backend": "^1.1.1",
    "knex": "^0.18.3",
    "koa": "^2.7.0",
    "koa-router": "^7.4.0",
    "koa-session": "^5.12.0",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.11",
    "nodemailer": "^6.2.1",
    "nunjucks": "^3.2.0",
    "validator": "^11.1.0",
    "winston": "^3.2.1"
  }
}
