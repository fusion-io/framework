{
  "name": "@fusion.io/framework",
  "version": "1.3.0-0",
  "description": "Fusion.IO Framework",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run build:watch\" \"npm run test:watch\"",
    "build": "babel src -d .",
    "build:watch": "nodemon --exec npm run build --watch src",
    "prepublish": "npm run build",
    "test:watch": "nodemon --exec npm run test --watch src ",
    "test": "mocha --exit --require=@babel/register"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "chai": "^4.2.0",
    "concurrently": "^4.1.1",
    "koa": "^2.7.0",
    "koa-router": "^7.4.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "sinon": "^7.3.2"
  },
  "dependencies": {
    "@fusion.io/container": "^2.0.10",
    "bcrypt": "^3.0.6",
    "koa-session": "^5.12.0",
    "lodash": "^4.17.11",
    "winston": "^3.2.1"
  }
}
